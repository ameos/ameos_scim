{
    "name": "ameos/ameos_scim",
    "description": "SCIM Extension for TYPO3",
    "type": "typo3-cms-extension",
    "require": {
        "typo3/cms-core": "^12.4 | dev-master",
        "php": "^8.0",
        "tmilos/scim-filter-parser": "^1.3"
    },
    "require-dev": {
        "typo3/testing-framework": "^8.0"
    },
    "replace": {
        "typo3-ter/scim": "self.version"
    },
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Ameos Team",
            "email": "typo3dev@ameos.com",
            "homepage": "http://www.ameos.com/"
        }
    ],
    "extra": {
        "typo3/cms": {
            "extension-key": "ameos_scim"
        }
    },
    "autoload": {
        "psr-4": {
            "Ameos\\AmeosScim\\": "Classes",
            "Ameos\\AmeosScim\\Tests\\": "Tests"
        }
    }
}